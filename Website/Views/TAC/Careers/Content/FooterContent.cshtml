@using Sitecore.Mvc
@using Sitecore.Mvc.Presentation
@using TAC.Utils
@model RenderingModel
@{
    var footerTextItem = Model.Item;
}
<div class="col-md-8">
    <div class="well">
        <h4>
            @Html.Sitecore().Field(Statics.FieldNames.ContentHeading, footerTextItem)
        </h4>
        <p>@Html.Sitecore().Field(Statics.FieldNames.ContentIntro, footerTextItem)</p>
    </div>
</div>